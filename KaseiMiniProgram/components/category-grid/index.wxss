/* components/category-grid/index.wxss */
.container {
  width: 100%;
  padding: 10rpx 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}

.inner-container {
  width: 730rpx;
  border: 1px dashed #DCEBE6
}

.grid-item {
  width: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.img {
  width: 60rpx;
  height: 60rpx;
}

.text {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #333;
}