<!--pages/home/home.wxml-->
<view>
  <image class="top-theme" src="{{themeA.entrance_img}}" mode="widthFix" bindload="topThemeBindload" style="height: {{topThemeHeight}}rpx;" alt=""></image>
  <swiper class="swiper" indicator-dots indicator-active-color="#157658" autoplay circular>
    <block wx:for="{{bannerB.items}}"  wx:key="*this">
      <swiper-item>
        <span></span>
        <image class="swiper" src="{{item.img}}" alt=""></image>
      </swiper-item>
    </block>
  </swiper>

  <k-category-grid grid="{{gridC}}"></k-category-grid>

  <image class="tactivity" src="{{activityD.entrance_img}}" alt=""></image>

</view>
